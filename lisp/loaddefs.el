	       ".dvi" ".toc" ".log" ".aux"
	       ".lof" ".brn" ".rnt" ".mem" ".lni" ".lis"
	       ".olb" ".tlb" ".mlb" ".hlb" ".glo" ".idx" ".lot" ".fmt"))
	    (t
	     '(".o" ".elc" "~" ".bin" ".lbin" ".fasl"
	       ".dvi" ".toc" ".log" ".aux" ".a" ".ln"
	       ".lof" ".blg" ".bbl" ".glo" ".idx" ".lot" ".fmt"))))

